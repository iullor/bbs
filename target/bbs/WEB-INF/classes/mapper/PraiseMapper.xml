<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.gyl.mapper.PraiseMapper">

    <insert id="add">
        insert into test_bbs.t_praise (post_id,user_id) values (#{userId},#{postId})
    </insert>
    <delete id="deletePaise">
        delete from test_bbs.t_praise where user_id=#{userId} and post_id =#{postId};
    </delete>
</mapper>